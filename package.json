{
	"name": "react-titan",
	"version": "1.1.0",
	"description": "A Lightly React UI Component Library",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"icon": "gulp -f gulpfile.js iconfont",
		"sass": "sass --no-source-map src/scss/index.scss:src/dist/main.css src/scss/theme.scss:src/dist/theme.css",
		"dev": "cross-env NODE_ENV=development webpack-dev-server",
		"build": "cross-env NODE_ENV=production npm run lang && webpack",
		"build:lib": "cross-env NODE_ENV=production npm run sass && npm run icon && rimraf ./lib && ttsc && copyfiles -u 1 ./src/dist/**/* ./lib",
		"prepare": "npm run build:lib",
		"lint": "eslint ."
	},
	"keywords": [
		"UI",
		"Component",
		"React",
		"Typescript"
	],
	"author": "lexuantunght",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "7.17.8",
		"@babel/node": "7.16.8",
		"@babel/plugin-transform-runtime": "7.18.6",
		"@babel/preset-env": "7.16.11",
		"@babel/preset-react": "7.16.7",
		"@babel/preset-typescript": "7.16.7",
		"@babel/runtime": "7.18.6",
		"@svgr/webpack": "^7.0.0",
		"@types/react": "^18.0.17",
		"@types/react-dom": "17.0.2",
		"@types/react-transition-group": "^4.4.5",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"babel-loader": "^9.1.2",
		"circular-dependency-plugin": "^5.2.2",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"eslint": "^8.36.0",
		"eslint-plugin-react": "^7.32.2",
		"file-loader": "^6.2.0",
		"gulp": "4.0.2",
		"gulp-consolidate": "0.2.0",
		"gulp-iconfont": "11.0.0",
		"html-webpack-plugin": "^5.5.1",
		"mini-css-extract-plugin": "^2.7.5",
		"rimraf": "4.4.0",
		"sass": "^1.59.3",
		"sass-loader": "^13.2.2",
		"style-loader": "^3.3.2",
		"terser-webpack-plugin": "^5.3.7",
		"ts-loader": "^9.4.2",
		"tsconfig-paths-webpack-plugin": "^4.0.1",
		"ttypescript": "^1.5.15",
		"typescript": "^4.9.3",
		"typescript-transform-paths": "^3.4.6",
		"webpack": "^5.81.0",
		"webpack-bundle-analyzer": "^4.8.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.13.3"
	},
	"dependencies": {
		"clsx": "^1.2.1",
		"rc-virtual-list": "^3.4.13",
		"react": "17.0.2",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "17.0.2",
		"react-transition-group": "^4.4.5"
	}
}
